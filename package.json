{
  "name": "code-clicker-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ericliucs.github.io/code-clicker-frontend",
  "dependencies": {
    "axios": "^1.8.3",
    "big.js": "^6.2.2",
    "howler": "^2.2.4",
    "immer": "^10.1.1",
    "numeral": "^2.0.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-modal": "^3.16.3",
    "react-scripts": "5.0.1",
    "tailwindcss": "^4.0.1",
    "use-immer": "^0.11.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"npm run watch:css\" \"npm run start\"",
    "watch:css": "npx @tailwindcss/cli -i ./src/App.css -o ./public/styles.css --watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "gh-pages": "^6.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
